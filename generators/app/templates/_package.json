{
  "name": "<%= pkg.name.slug %>",
  "version": "<%= pkg.version %>",
  "description": "<%= pkg.desc %>",
  "repository": "<%= pkg.repo %>",
  "license": "<%= pkg.license %>",
  "keywords": [
  <%
  if (typeof pkg.keywords === 'Array') {
    for (var p = 0; ((p < pkg.keywords.length) && (pkg.keywords[p] !== '')); p++) {
      if (p !== (pkg.keywords.length - 1)) {
  -%>
  "<%= pkg.keywords[p] %>",
  <%  } else { -%>
  "<%= pkg.keywords[p] %>"
  <%  }
    }
  }
  -%>],
  "author": {
    "name": "<%= author.name %>",
    "email": "<%= author.email %>",
    "url": "<%= author.home %>"
  },
  "dependencies": {
  },
  "devDependencies": {
    "angular": "~1.5.8",
    "angular-cookies": "~1.5.8",
    "angular-mocks": "~1.5.8",
    "angular-resource": "~1.5.8",
    "angular-route": "~1.5.8",
    "angular-sanitize": "~1.5.8",
    "eslint": "~3.1.1",
    "gulp": "~3.9.0",
    "gulp-eslint": "~3.0.1",
    "gulp-exclude-gitignore": "~1.0.0"
  }
}
